{
  "name": "a5-logo-render",
  "author": "Raoul Zander",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "css:autoprefixer": "postcss --no-map -u autoprefixer -r dist/*.css",
    "css:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js --output-style compressed -o dist src/scss",
    "build:css": "npm run css:scss && npm run css:autoprefixer",
    "build:js": "webpack --mode=production",
    "build:html": "posthtml -c posthtml.json",
    "build:images": "imagemin src/images/**/* --out-dir=dist/images",
    "//build:images": "imagemin src/images/**/* --out-dir=dist/images --plugin=svgo",
    "build": "run-s build:*",
    "serve": "browser-sync start --server \"dist\" --files \"dist\"",
    "watch:css": "onchange \"src/scss\" -- npm run build:css",
    "watch:html": "onchange \"src/views\" -- npm run build:html",
    "watch:images": "onchange \"src/images\" -- npm run build:images",
    "watch:js": "onchange \"src/ts\" -- webpack --mode=development",
    "watch": "run-p serve watch:*"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "@popperjs/core": "^2.9.1",
    "bootstrap": "^5.0.0-beta2",
    "save-svg-as-png": "^1.4.17"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@types/bootstrap": "^5.0.8",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.14",
    "eslint": "^7.21.0",
    "eslint-loader": "^4.0.2",
    "htmlnano": "^0.2.8",
    "imagemin-cli": "^6.0.0",
    "imagemin-svgo": "^9.0.0",
    "node-sass": "^5.0.0",
    "node-sass-package-importer": "^5.3.2",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss": "^8.2.8",
    "postcss-cli": "^8.3.1",
    "posthtml": "^0.15.1",
    "posthtml-cli": "^0.9.1",
    "posthtml-inline-svg": "^0.2.0",
    "posthtml-modules": "^0.7.4",
    "source-map-loader": "^2.0.1",
    "typescript": "^4.2.3",
    "webpack": "^5.24.4",
    "webpack-cli": "^4.5.0"
  }
}
